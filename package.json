{
  "name": "@dschz/polymarket-clob-client",
  "description": "Fork of Polymarket CLOB Client with optimized bundle size and enhanced Web3 integration",
  "version": "0.4.4",
  "author": "Daniel Sanchez <dsanc89@pm.me>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/dsnchz/polymarket-clob-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dsnchz/polymarket-clob-client.git"
  },
  "bugs": {
    "url": "https://github.com/dsnchz/polymarket-clob-client/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blockchain",
    "browser",
    "client",
    "clob",
    "crypto",
    "dapp",
    "defi",
    "eip1193",
    "eip6963",
    "ethereum",
    "ethers",
    "metamask",
    "orderbook",
    "polygon",
    "polymarket",
    "prediction-markets",
    "privy",
    "trading",
    "typescript",
    "wallet",
    "walletconnect",
    "web3"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "deploy": "bun run build && bun publish",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pkg:changeset": "changeset",
    "pkg:version": "changeset version",
    "pkg:publish": "bun run build && changeset publish",
    "test": "bun test",
    "test:cov": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@eslint/js": "9.38.0",
    "@tsconfig/strictest": "2.0.7",
    "@types/bun": "1.3.1",
    "@types/jsdom": "27.0.0",
    "@types/node": "22.18.4",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "dotenv": "17.2.3",
    "eslint": "9.35.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "jiti": "2.6.1",
    "jsdom": "27.0.1",
    "path": "0.12.7",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "ws": "8.18.3"
  },
  "dependencies": {
    "@dschz/polymarket-builder-signing-sdk": "^0.3.0",
    "@dschz/polymarket-clob-order-utils": "^0.2.1",
    "@ethersproject/bignumber": "^5.8.0",
    "@ethersproject/constants": "^5.8.0",
    "@ethersproject/contracts": "^5.8.0",
    "@ethersproject/providers": "^5.8.0",
    "@ethersproject/units": "^5.8.0",
    "@ethersproject/wallet": "^5.8.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "bun@>=1.3.0"
}
